    "jsdom": "^26.1.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "start-server-and-test": "^2.0.11",
    "typescript": "~5.8.3",
    "vite": "^6.3.2",
    "vite-plugin-vue-devtools": "^7.7.5",
    "vitest": "^3.1.2",
    "vue-tsc": "^2.2.8"
